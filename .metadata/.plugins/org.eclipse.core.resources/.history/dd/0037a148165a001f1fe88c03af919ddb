package pooExcercicies2;

public class ContaBancaria {
	String numeroConta;
	String titular;
	int saldo;
	
	public ContaBancaria(String numeroConta, String titular, String saldo) {
		super();
		this.numeroConta = numeroConta;
		this.titular = titular;
		this.saldo = saldo;
	}


	public void depositar(int saldo) {
		this.saldo += saldo;
	}
	
	
}
